{
    "name": "user",
    "displayName": "User Tweaks",
    "description": "Custom modifications",
    "version": "0.0.1",
    "publisher": "robbert-vdh",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:user.ipython.send"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "user.ipython.send",
                "title": "Send to IPython"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/copy-paste": "^1.1.30",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "copy-paste": "^1.3.0"
    }
}
